#import "UITextField+Ex.h"

@implementation UITextField (Ex)
-(instancetype)initWithFont:(UIFont *) aFont andColor:(UIColor *)aColor{
    if (self=[super init]) {
        self.textColor=aColor;
        self.font=aFont;
    }
    
    return self;
}

@end
