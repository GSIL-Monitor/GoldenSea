#import <UIKit/UIKit.h>

typedef NS_ENUM(NSUInteger, RCSNavBarColorStyle) {
    RCSNavBarColorStyleDark = 0,
    RCSNavBarColorStyleLightRed = 1,
    RCSNavBarColorStyleDarkTranslucent = 2,
    RCSNavBarColorStyleLightRedTranslucent = 3
};

@interface UINavigationController (backColorStyle)
- (void)setNavigationBarColorStyle:(RCSNavBarColorStyle)colorStyle;
@end
